    "drizzle-orm": "^0.44.4",
    "farming-simulator-types": "^1.8.0",
    "ftp": "^0.3.10",
    "lodash": "^4.17.21",
    "ms": "^2.1.3",
    "node-cron": "^3.0.3",
    "polka": "^0.5.2",
